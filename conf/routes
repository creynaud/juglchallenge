# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.email(q: Option[String])
POST    /enonce/2                   controllers.Application.markdown
GET     /enonce                     controllers.Application.getMarkdown

POST    /minesweeper/resolve        controllers.Application.solveMineSweeper

POST    /diet/resolve               controllers.Application.solveDiet
GET     /activities                 controllers.Application.activities
